{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules",
    "src/js/client",
    "src/js/test"
  ],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "src/js/server",
    "src/js/shared",
    "server.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}